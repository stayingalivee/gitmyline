{
  "name": "gitmyline",
  "displayName": "gitmyline",
  "description": "",
  "icon": "gitmyline-icon.png",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/stayingalivee/gitmyline.git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:gitmylineActivate"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gitmyline",
        "title": "gitmyline"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "./node_modules/typescript/bin/tsc -p ./",
    "watch": "./node_modules/typescript/bin/tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.70.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@vscode/vsce": "^2.22.0"
  }
}
